    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "concurrently": "^4.1.0",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "jsonwebtoken": "8.5.0",
    "mongoose": "5.4.15",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "multer-s3": "2.9.0",
    "nodemailer": "5.1.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "validator": "10.11.0"
